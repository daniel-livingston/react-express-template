{
	"name": "create-react-express",
	"version": "1.0.0",
	"description": "",
	"main": "index.js",
	"scripts": {
		"build": "webpack --config=webpack.production.config.js",
		"dev": "concurrently \"npm run prettier-watch\" \"npm run dev:build\" \"npm run dev:start\"",
		"dev:build": "webpack --config=webpack.development.config.js --watch --progress",
		"dev:start": "env-cmd -f ./config/dev.env nodemon server/index",
		"prettier": "prettier --write --ignore-unknown --config=.prettierrc.json",
		"prettier-watch": "onchange \"**/*\" -- npm run prettier {{changed}}",
		"start": "node server/index",
		"test": "jest --config=jest.config.json",
		"test-watch": "jest --config=jest.config.json --watch"
	},
	"author": "",
	"license": "ISC",
	"dependencies": {
		"express": "^4.17.1",
		"prop-types": "^15.7.2",
		"react": "^17.0.1",
		"react-dom": "^17.0.1",
		"react-router-dom": "^5.2.0"
	},
	"devDependencies": {
		"@babel/core": "^7.12.3",
		"@babel/plugin-proposal-class-properties": "^7.12.1",
		"@babel/plugin-transform-runtime": "^7.12.1",
		"@babel/preset-env": "^7.12.1",
		"@babel/preset-react": "^7.12.5",
		"babel-jest": "^26.6.3",
		"babel-loader": "^8.2.1",
		"concurrently": "^5.3.0",
		"css-loader": "^5.0.1",
		"env-cmd": "^10.1.0",
		"eslint": "^7.13.0",
		"eslint-config-prettier": "^6.15.0",
		"jest": "^26.6.3",
		"mini-css-extract-plugin": "^1.3.1",
		"nodemon": "^2.0.6",
		"onchange": "^7.1.0",
		"prettier": "^2.1.2",
		"react-test-renderer": "^17.0.1",
		"sass": "^1.29.0",
		"sass-loader": "^10.1.0",
		"supertest": "^6.0.1",
		"webpack": "^5.4.0",
		"webpack-cli": "^4.2.0"
	}
}
